#! /usr/bin/make -f

## See debhelper(7) (uncomment to enable).
## Output every command that modifies files on the build system.
export DH_VERBOSE = 1

override_dh_prep:
	./nvidia-installer.run  --extract-only
	mkdir -p debian/tmp
	ls -l ./
	cp -rvf ./NVIDIA-Linux-*/* debian/tmp
	ls -l debian/tmp
	
%:
	dh $@
